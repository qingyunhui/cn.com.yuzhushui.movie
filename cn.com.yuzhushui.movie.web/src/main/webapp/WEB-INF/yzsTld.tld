		<name>getNameByCode</name>
		<function-class>qing.yun.hui.common.utils.EnumUtil</function-class>
		<function-signature>java.lang.String  getNameByCode(java.lang.String,java.lang.Object)</function-signature>
		<example>${yzsTld:getNameByCode(x,y)}</example>
	</function>
	<function>
		<name>getNameByValue</name>
		<function-class>qing.yun.hui.common.utils.EnumUtil</function-class>
		<function-signature>java.lang.String getNameByValue(java.lang.String,java.lang.String)</function-signature>

		<name>getSysAccount</name>
		<function-class>cn.com.yuzhushui.movie.common.util.SessionUtil</function-class>
		<function-signature>cn.com.yuzhushui.movie.sys.biz.entity.SysAccount getSysAccount()</function-signature>
		<example>${yzsTld:getSysAccount()}</example>
	</function>
	<function>
		<name>getSysUser</name>
		<function-class>cn.com.yuzhushui.movie.common.util.SessionUtil</function-class>
